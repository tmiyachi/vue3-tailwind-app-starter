{
  "name": "vue3-tailwind-app-starter",
  "version": "1.0.3",
  "description": "The application template for Vue.js 3.x wit tailwindcss",
  "dependencies": {
    "tailwindcss": "^3.4.17",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "eslint-plugin-vue": "^9.32.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "postcss-html": "^1.7.0",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.4.2",
    "rollup-plugin-license": "^3.5.3",
    "stylelint": "^16.12.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "vite": "^5.4.11"
  },
  "scripts": {
    "build": "vite build",
    "build:dev": "webpack --config webpack.dev.js --watch",
    "start": "vite dev",
    "lint:eslint": "eslint src/**/*.{js,vue}",
    "lint:stylelint": "stylelint src/**/*.{css,scss,vue}",
    "lint:prettier": "prettier src/**/*.{js,css,scss,vue} --check",
    "lint": "run-p lint:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-s fix:*"
  },
  "type": "module",
  "main": "./src/index.js",
  "private": true,
  "keywords": [],
  "author": "tmiyachi",
  "license": "MIT"
}
