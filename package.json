{
  "name": "vue3-tailwind-app-starter",
  "version": "1.0.2",
  "description": "The application template for Vue.js 3.x wit tailwindcss",
  "dependencies": {
    "core-js": "^3.32.0",
    "tailwindcss": "^3.3.3",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@vue/compiler-sfc": "^3.3.4",
    "autoprefixer": "^10.4.15",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.47.0",
    "eslint-cli": "^1.1.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-vue": "^9.17.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.28",
    "postcss-html": "^1.5.0",
    "postcss-loader": "^7.3.3",
    "prettier": "^3.0.2",
    "stylelint": "^15.10.2",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.3.0",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "vue-loader": "^17.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "build:dev": "webpack --config webpack.dev.js --watch",
    "start": "webpack serve --open --config webpack.dev.js",
    "lint:eslint": "eslint src/**/*.{js,vue}",
    "lint:stylelint": "stylelint src/**/*.{css,scss,vue}",
    "lint:prettier": "prettier src/**/*.{js,css,scss,vue} --check",
    "lint": "run-p lint:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-s fix:*"
  },
  "private": true,
  "keywords": [],
  "author": "tmiyachi",
  "license": "MIT"
}
